beta0       <- 0
beta1       <- 1
beta2       <- 1
noisy.data  <- list()

for ( i in 1:N)
{
  f1_obs <- f1
  f2_obs <- f2
  noise <- rnorm(length(f1), sd= 2)
  noisy.data [[i]] <-   beta1*G[i,pos1]*f1_obs + beta2*G[i,pos2]*f2_obs + noise
  
}
noisy.data <- do.call(rbind, noisy.data)

 

Y <- noisy.data
X <- G
#Running fSuSiE

out <- susiF(Y,X,L=6  ,maxit=10)
out$alpha