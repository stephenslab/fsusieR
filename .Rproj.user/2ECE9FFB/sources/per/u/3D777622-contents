a <- sample(letters)


pos<- 1300+1:120*10#~you datra start at pos 1310 and end at pos 2500 and have 250 time points
start_pos <- min( pos)
end_pos <-max(pos)
Y <- matrix(rnorm(length(pos)*10), nrow = 10)
inter_pol.obj <- interpol_mat(Y, pos)
x <- start_pos + (end_pos-start_pos)/(length(pos))*inter_pol.obj$grid 

plot( x, susif.obj$fitted_func [[1]], type ="l")
lines()

                                 